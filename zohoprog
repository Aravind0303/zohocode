#include<stdio.h>
int main()
{
int row,i,rep=0,count=1,k;
scanf("%d",&row);
for(i=1;i<=row;i++)
{
while(i%2==0)
{
rep=i*row;
count=rep;
for(k=0;k<row;k++)
{
printf("%d",rep);
rep--;
}
count++;
printf("\n");
break;
}
while(i%2!=0)
{
for(k=0;k<row;k++)
{
printf("%d",count);
count++;
}
printf("\n");
break;
}  
}
}
